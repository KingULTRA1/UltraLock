{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "chrome": "readonly"
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn"],
    "no-console": ["off"],
    "semi": ["error", "always"]
  }
}